using System;

namespace Lista2
{
    class Program
    {

        static void Main(string[] args)
        {
            double numero1;
            double numero2;
            double numero3;

            Console.WriteLine("Digite um numero: ");
            numero1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Digite um numero: ");
            numero2 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Digite um numero: ");
            numero3 = Convert.ToDouble(Console.ReadLine());


            if (numero1 < numero2)
            {
                if (numero2<numero3)
                {
                    Console.WriteLine("Seus numeros em ordem crescente sao: " + numero3 + " " + numero2 + " " + numero1);
                }
            }
            if (numero2 < numero3)
            {
                if (numero3 < numero1)
                {
                    Console.WriteLine("Seus numeros em ordem crescente sao: " + numero1 + " " + numero3 + " " + numero2);
                }
            }
            if (numero3 < numero1)
            {
                if (numero1 < numero2)
                {
                    Console.WriteLine("Seus numeros em ordem crescente sao: " + numero2 + " " + numero1 + " " + numero3);
                }
            }
            if (numero1 < numero3)
            {
                if (numero3 < numero2)
                {
                    Console.WriteLine("Seus numeros em ordem crescente sao: " + numero2 + " " + numero3 + " " + numero1);
                }
            }
            if (numero2 < numero1)
            {
                if (numero1 < numero3)
                {
                    Console.WriteLine("Seus numeros em ordem crescente sao: " + numero3 + " " + numero1 + " " + numero2);
                }
            }
            if (numero3 < numero2)
            {
                if (numero2 < numero1)
                {
                    Console.WriteLine("Seus numeros em ordem crescente sao: " + numero1 + " " + numero2 + " " + numero3);
                }
            }

            Console.ReadLine();

        }
    }
} 
