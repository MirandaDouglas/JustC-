using System;

namespace Lista2
{
    class Program
    {

        static void Main(string[] args)
        {
            //1x1=1
            for (int i = 0; i <= 10; i++) 
            {
                for(int j = 1; j<=10;j++)
                {
                    int r = i * j;
                    Console.WriteLine( i + "x" + j + "=" +r);
                }

            }
            Console.WriteLine("Fim do Programa");
        }
    }
}
